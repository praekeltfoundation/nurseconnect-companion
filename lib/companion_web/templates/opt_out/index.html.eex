<table class="table">
  <thead>
    <tr>
      <th>Contact ID</th>
      <th>Created</th>
    </tr>
  </thead>
  <tbody>
<%= for optout <- @optouts do %>
    <tr>
      <td><%= optout.contact_id %></td>
      <td><%= format_timestamp optout.inserted_at %></td>
    </tr>
<% end %>
  </tbody>
</table>
<div>
<%= paginate @conn, @kerosene %>
</div>
